import ChargeSection5 from "../components/ChangeSeccion5.astro";
import ChargeSection4 from "../components/ChangeSeccion4.astro";
import ChargeSection3 from "../components/ChangeSeccion3.astro";
import ChargeSection2 from "../components/ChangeSeccion2.astro";
import ChargeSection1 from "../components/ChargeSection.astro";
