   <div class="snap-center">
    <HeroSection/>
   </div>
   <div class="snap-center">
    <ChargeSection/>
   </div>
   <div class="snap-center">
    <ChargeSection1/>
   </div>
   <div class="snap-center">
    <ChargeSection2/>
   </div>
   <div class="snap-center">
    <ChargeSection3/>
   </div>
   <div class="snap-center">
    <ChargeSection4/>
   </div>
   <div class="snap-center">
    <EndSection/>
   </div>

  </main>
</Layout>
